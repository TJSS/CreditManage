<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>	<!-- Created by the tjuscs ncfxy -->
	<class name="com.pingancar.creditmanage.pojo.ShopInfoPojo" table="shopinfo" lazy="true">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"/>
		</id>

		<property name="shopid" type="java.lang.String" column="shopid" length="20" />

		<property name="paserviceinfoid" type="java.lang.String" column="paserviceinfoid" length="20" />

		<property name="name" type="java.lang.String" column="name" length="255" />

		<property name="location" type="java.lang.String" column="location" length="255" />

		<property name="xpos" type="java.lang.Double" column="xpos" length="22" />

		<property name="ypos" type="java.lang.Double" column="ypos" length="22" />

		<property name="interf" type="java.lang.String" column="interf" length="255" />

		<property name="tag1" type="java.lang.String" column="tag1" length="20" />

		<property name="tag2" type="java.lang.String" column="tag2" length="20" />

		<property name="con" type="java.lang.String" column="con" length="11" />

		<property name="status" type="java.lang.String" column="status" length="20" />

	</class>
</hibernate-mapping>