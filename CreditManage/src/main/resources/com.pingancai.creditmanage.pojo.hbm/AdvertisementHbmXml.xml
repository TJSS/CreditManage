<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>	<!-- Created by the tjuscs ncfxy -->
	<class name="org.tju.scs.damai.innovationFund.pojo.AdvertisementPojo" table="advertisement" lazy="true">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"/>
		</id>

		<property name="adid" type="java.lang.String" column="adid" length="20" />

		<property name="imgurl" type="java.lang.String" column="imgurl" length="255" />

	</class>
</hibernate-mapping>