<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>	<!-- Created by the tjuscs ncfxy -->
	<class name="com.pingancar.creditmanage.pojo.AdminPojo" table="admin" lazy="true">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"/>
		</id>

		<property name="username" type="java.lang.String" column="username" length="10" />

		<property name="passwd" type="java.lang.String" column="passwd" length="20" />

	</class>
</hibernate-mapping>