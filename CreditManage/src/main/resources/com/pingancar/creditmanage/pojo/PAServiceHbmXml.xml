<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>	<!-- Created by the tjuscs ncfxy -->
	<class name="com.pingancar.creditmanage.pojo.PAServicePojo" table="paservice" lazy="true">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"/>
		</id>

		<property name="paserviceinfoid" type="java.lang.String" column="paserviceinfoid" length="20" />

		<property name="paserviceid" type="java.lang.String" column="paserviceid" length="20" />

		<property name="type" type="java.lang.String" column="type" length="20" />

		<property name="tag1" type="java.lang.String" column="tag1" length="20" />

		<property name="leftnum" type="java.lang.Integer" column="leftnum" length="11" />

		<property name="amount" type="java.lang.Integer" column="amount" length="11" />

		<property name="creditcost" type="java.lang.Double" column="creditcost" length="22" />

	</class>
</hibernate-mapping>